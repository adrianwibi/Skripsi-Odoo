<?xml version='1.0' encoding='utf-8'?>
<odoo>

   <record id="unsika_user_category"  model="ir.module.category">
      <field name="name">Kategori Pengguna Unsika</field>
      <field name="sequence">1</field>
      <field name="visible" eval="True" />
      </record>

   <record id="group_mahasiswa" model="res.groups">
       <field name="name">Mahasiswa</field>
       <field name="category_id" ref="unsika_user_category"/>
   </record>

   <record id="group_dosen" model="res.groups">
       <field name="name">Dosen</field>
       <field name="category_id" ref="unsika_user_category"/>
   </record>

</odoo>